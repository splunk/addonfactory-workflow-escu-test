{
    "extends": [
        "config:base",
        "group:all",
        ":semanticCommitTypeAll(chore)",
        ":prHourlyLimit2"
    ],
    "force": {
        "constraints": {
            "python": ">=3.7,<3.8",
            "poetry": "1.5.1"
        },
        "constraintsFiltering": "strict"
    },
    "schedule": [
        "before 4am on the first day of the month"
    ],
    "lockFileMaintenance": {
        "enabled": true,
        "schedule": [
            "before 4am on the first day of the month"
        ]
    },
    "enabledManagers": [
        "poetry",
        "npm",
        "gradle"
    ],
    "rangeStrategy": "pin",
    "prConcurrentLimit": 5,
    "packageRules": [
        {
            "matchPackageNames": [
                "urllib3"
            ],
            "allowedVersions": "<2.0.0"
        }
    ],
    "ignoreDeps": [
        "splunk-add-on-ucc-modinput-test"
    ]
}
